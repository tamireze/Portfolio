{
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  },
    "compilerOptions": {
      "target": "es5",
      "lib": ["es5", "dom"],
      "types": ["cypress", "node"],
      "paths": {
        "@badeball/cypress-cucumber-preprocessor/*": ["./node_modules/@badeball/cypress-cucumber-preprocessor/dist/subpath-entrypoints/*"]
      },
      "esModuleInterop": true,
      "moduleResolution": "node",
      "module": "CommonJS",
    },
    "include": ["**/*.ts"]
  }